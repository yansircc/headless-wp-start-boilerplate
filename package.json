{
  "name": "headless-wp-start-boilerplate",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3008",
    "build": "bun validate && vite build",
    "preview": "vite preview",
    "deploy": "bun run build && wrangler deploy",
    "deploy:preview": "bun run build && wrangler deploy --env preview",
    "cf-typegen": "wrangler types",
    "env:push": "wrangler secret bulk .env.prod.local",
    "test": "vitest run",
    "check": "ultracite check .",
    "lint": "biome check --write .",
    "lint:fix": "biome check --fix --unsafe .",
    "typecheck": "tsc --noEmit",
    "sync": "bun scripts/sync.ts",
    "seo": "bun scripts/generate-seo-files.ts",
    "seo:check": "bun scripts/generate-seo-files.ts --check",
    "validate": "bun scripts/validate.ts",
    "validate:check": "bun scripts/validate.ts --check",
    "prepare": "husky"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.10",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "graphql": "^16.12.0",
    "graphql-request": "5",
    "lucide-react": "^0.544.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwindcss": "^4.0.6",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@cloudflare/vite-plugin": "^1.19.0",
    "@graphql-codegen/cli": "^6.1.0",
    "@graphql-codegen/client-preset": "^5.2.1",
    "@graphql-codegen/near-operation-file-preset": "^4.0.0",
    "@graphql-codegen/typed-document-node": "^6.1.4",
    "@graphql-codegen/typescript": "^5.0.6",
    "@graphql-codegen/typescript-operations": "^5.0.6",
    "@graphql-tools/graphql-file-loader": "^8.1.8",
    "@graphql-tools/load": "^8.1.7",
    "@graphql-tools/schema": "^10.0.30",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "bun-types": "^1.3.4",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.7",
    "typescript": "^5.7.2",
    "ultracite": "6.4.1",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "wrangler": "^4.56.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "biome check --write"
    ],
    "*.{json,md}": [
      "biome format --write"
    ]
  }
}
